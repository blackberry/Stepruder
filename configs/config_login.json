{
	"ssl": true,
	"port": 443,
	"grep_last_response": "Invalid credentials",
	"request_separator": "#####",
	"proxies": {
		"http": "https://127.0.0.1:8080",
		"https": "https://127.0.0.1:8080"
	},
	"substitutions": {
		"responsevars": {
		  "CLIENTID": {
			"regex": "clientId.*?:\\s*(\\d+)"
		  },
		  "SYNCID": {
			"regex": "syncId.*?:\\s*(\\d+)"
		  },
		  "CSRFTOKEN": {
			"regex": "Vaadin-Security-Key.*?:\\\\\"(.*?)\\\\"
		  },
		  "#JSESSIONID": {
			"regex": "JSESSIONID=(.*?)(?:;|$)"
		  }
		}
	},
	"payloads": {
		"PAYLOAD1": {"type": "list", "path": "payloads/payloads_sqli.txt"}
	}
}